      "$ref": "./gh.csharp.schema.json"
    },
    "gh.ghpython": {
      "$ref": "./gh.ghpython.schema.json"
    },
    "gh.ironpython": {
      "$ref": "./gh.ironpython.schema.json"
    },
    "gh.numberslider": {
      "$ref": "./gh.numberslider.schema.json"
    },
    "gh.panel": {
      "$ref": "./gh.panel.schema.json"
    },
    "gh.python": {
      "$ref": "./gh.python.schema.json"
    },
    "gh.scribble": {
      "$ref": "./gh.scribble.schema.json"
    },
    "gh.toggle": {
      "$ref": "./gh.toggle.schema.json"
    },
    "gh.valuelist": {
      "$ref": "./gh.valuelist.schema.json"
    },
    "gh.vbscript": {
      "$ref": "./gh.vbscript.schema.json"
    }
